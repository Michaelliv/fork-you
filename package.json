{
  "name": "fork-you",
  "version": "0.1.0",
  "description": "A CLI tool",
  "module": "src/main.ts",
  "type": "module",
  "bin": {
    "fu": "./src/main.ts"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "dev": "bun run src/main.ts",
    "build": "bun build --compile src/main.ts --outfile fu",
    "build:linux": "bun build --compile --target=bun-linux-x64 src/main.ts --outfile fu-linux-x64",
    "build:mac-arm": "bun build --compile --target=bun-darwin-arm64 src/main.ts --outfile fu-darwin-arm64",
    "build:mac-x64": "bun build --compile --target=bun-darwin-x64 src/main.ts --outfile fu-darwin-x64",
    "test": "bun test",
    "format": "bunx biome format --write src/",
    "lint": "bunx biome lint src/",
    "check": "bunx biome check src/"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/sql.js": "^1.4.9",
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.6.2",
    "nanoid": "^5.1.6",
    "sql.js": "^1.14.0"
  }
}
